<Window x:Class="Prototype.wndViewHouseholdInfo"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Prototype"
        mc:Ignorable="d"
        Title="wndViewHouseholdInfo" Height="450" Width="800">
    <Grid Background="#FFBCBEBF">
        <TextBox x:Name="nameHouse" HorizontalAlignment="Center" Margin="0,103,0,0" Text="TextBox" TextWrapping="Wrap" VerticalAlignment="Top" Width="162" Height="28" TextChanged="TextBox_TextChanged"/>
        <ListBox x:Name="lstHouse" Margin="100,190,100,37" />
        <Label Content="Enter the last name of the household: " HorizontalAlignment="Center" Margin="0,49,0,0" VerticalAlignment="Top" FontSize="20"/>
        <Label Content="Household details:&#xD;&#xA; " HorizontalAlignment="Center" Margin="0,152,0,0" VerticalAlignment="Top" FontSize="20"/>

    </Grid>
</Window>
